<template name="3e6b220c">
  <view class="_div data-v-37b6c60c container flex">
    <view class="_div data-v-37b6c60c room {{lengthOfRemoteVideo <= 3 ? '' : 'scroll'}}">
      <view class="_div data-v-37b6c60c video flex-center {{previewVideo ? '' : 'opacity55'}}">
        <video hidden="{{!(previewVideo)}}" id="previewVideo" muted autoplay playsinline controls
               class="_video data-v-37b6c60c"></video>
        <image hidden="{{!(!previewVideo)}}" src="/static/roomset_1.png" alt class="_img data-v-37b6c60c"></image>
      </view>
      <view class="_div data-v-37b6c60c video flex-center {{r ? '' : 'opacity55'}}"
            hidden="{{!(lengthOfRemoteVideo<=3? i<3: i<lengthOfRemoteVideo)}}" wx:for="{{remoteVideo}}" wx:for-index="i"
            wx:for-item="r">
        <video hidden="{{!(r)}}" class="_video data-v-37b6c60c remoteVideo" autoplay playsinline controls></video>
        <image hidden="{{!(!r)}}" src="{{'/static/roomset_' + (i+2) + '.png'}}" alt
               class="_img data-v-37b6c60c"></image>
      </view>
    </view>
    <view class="_div data-v-37b6c60c bottom">
      <image src="{{'/static/camera' + (enableCamera ? '' : '-dis')  + '.png'}}" bindtap="handleProxy" alt
             data-eventid="{{'0'}}" data-comkey="{{$k}}" class="_img data-v-37b6c60c"></image>
      <image src="{{'/static/mic'+ (enableMic ? '' : '-dis') +'.png'}}" bindtap="handleProxy" alt data-eventid="{{'1'}}"
             data-comkey="{{$k}}" class="_img data-v-37b6c60c"></image>
    </view>
  </view>
</template>
